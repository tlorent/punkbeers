{
  "name": "punk-beers",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.0.0",
    "@tailwindcss/postcss7-compat": "^2.0.2",
    "@types/react-router-dom": "^5.1.7",
    "autoprefixer": "^9.8.6",
    "postcss": "^7.0.35",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.2"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "typecheck": "tsc --project . --noEmit"
  },
  "eslintConfig": {
    "extends": "tlorent"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "eslint": "^7.18.0",
    "eslint-config-tlorent": "^1.0.4",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run typecheck && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}?(x)": [
      "eslint --fix",
      "prettier --write"
    ]
  }
